        "import os\n",
        "import google.generativeai as genai\n",
        "from tavily import TavilyClient\n",
        "from langchain_core.documents import Document\n",
        "from langchain_community.document_loaders import WebBaseLoader\n",
        "\n",
        "# Set API keys\n",
        "os.environ[\"GOOGLE_API_KEY\"] = \"AIzaSyA3ZELaAWkKLIDv-oNGvyBxnR5W8cqB8Zs\"\n",
        "os.environ[\"TAVILY_API_KEY\"] = \"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\"\n",
        "\n",
        "# Configure Gemini\n",
        "genai.configure(api_key=os.environ[\"GOOGLE_API_KEY\"])\n",
        "gemini_model = genai.GenerativeModel(\"gemini-2.5-flash\")\n",
        "\n",
        "# Tavily search\n",
        "tavily = TavilyClient(api_key=os.environ[\"TAVILY_API_KEY\"])\n",
        "\n",
        "question = \"Recent trends in brain-computer interface research\"\n",
        "\n",
        "search_results = tavily.search(query=question, max_results=5)\n",
        "\n",
        "print(\" Raw search_results:\\n\", search_results)\n",
        "\n",
        "# Extract URLs\n",
        "urls = [item[\"url\"] for item in search_results[\"results\"]]\n",
        "print(\" Extracted URLs:\\n\", urls)\n",
        "\n",
        "# Load documents\n",
        "loader = WebBaseLoader(urls)\n",
        "docs = loader.load()\n",
        "print(f\" Loaded {len(docs)} documents\")\n"
      ],
