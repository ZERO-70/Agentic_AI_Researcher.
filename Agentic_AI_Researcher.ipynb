        "#Web Scrap through Article"
      ],
      "metadata": {
        "id": "F2ycemU5ynPz"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "from newspaper import Article\n",
        "from langchain.schema import Document\n",
        "\n",
        "def scrape_articles_combined(urls):\n",
        "    docs = []\n",
        "    for url in urls:\n",
        "        article = Article(url)\n",
        "        article.download()\n",
        "        article.parse()\n",
        "\n",
        "        title = article.title.strip()\n",
        "        text = article.text.strip()\n",
        "\n",
        "        full_text = f\"* {title} *: {text}\"\n",
        "\n",
        "        docs.append(Document(\n",
        "            page_content=full_text,\n",
        "            metadata={\"title\": title, \"source\": url}\n",
        "        ))\n",
        "    return docs\n"
      ],
      "metadata": {
        "id": "_Tm-b2cZ2NBY"
      },
      "execution_count": 6,
      "outputs": []
